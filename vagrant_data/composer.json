{
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.cakephp.org"
        }
    ],
    "require": {
        "cakephp/cakephp": "2.4.*",
        "cakephp/debug_kit": "~2.2",
        "cakedc/migrations": "~2.3",
        "cakedc/search": "~2.5",
        "cakedc/utils": "~1.4",
        "cakedc/users": "~2.1",
        "aws/aws-sdk-php": "2.7.5",
        "tecnick.com/tcpdf": "6.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/phpunit-selenium": "*",
        "phpunit/dbunit": "*",
        "phpunit/phpunit-story": "*",
        "phpunit/phpunit-skeleton-generator": "*",
        "phpmd/phpmd": "~2.1",
        "apigen/apigen": "~4.0",
        "squizlabs/php_codesniffer": "~1.4",
        "cakephp/cakephp-codesniffer": "~1.0"
    },
    "config": {
        "vendor-dir": "Vendor/"
    }
